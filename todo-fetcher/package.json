{
  "name": "todo-fetcher",
  "private": true,
  "version": "0.0.0",
  "repository": "https://github.com/Effect-TS/docs/tree/code-examples",
  "author": "Michael Arnaldi",
  "license": "MIT",
  "description": "An example app that fetches todo from the JSON Placeholder API",
  "type": "module",
  "packageManager": "yarn@3.2.1",
  "scripts": {
